{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@types/qs": "^6.9.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.10",
    "apollo-boost": "^0.4.7",
    "braft-editor": "^2.3.9",
    "braft-extensions": "^0.1.0",
    "draft-js-prism": "^1.0.6",
    "dva-logger": "^1.0.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "highlight.js": "^9.18.1",
    "lint-staged": "^10.0.7",
    "marked": "^0.8.2",
    "prettier": "^1.19.1",
    "prismjs": "^1.19.0",
    "qs": "^6.9.1",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "umi": "^3.0.10",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-ui": "^2.0.9"
  }
}
